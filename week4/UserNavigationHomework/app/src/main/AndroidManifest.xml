<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:dist="http://schemas.android.com/apk/distribution"
    package="com.example.usernavigationhomework">

    <dist:module dist:instant="true" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.usernavigationhomework.donut"
            android:label="Donut"
            android:parentActivityName="com.example.usernavigationhomework.MainActivity">
            <meta-data android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.usernavigationhomework.MainActivity"/>
        </activity>
        <activity android:name="com.example.usernavigationhomework.icecream"
            android:label="Ice-Cream"
            android:parentActivityName="com.example.usernavigationhomework.MainActivity">
            <meta-data android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.usernavigationhomework.MainActivity"/>
        </activity>
        <activity android:name="com.example.usernavigationhomework.froyo"
            android:label="Froyo"
            android:parentActivityName="com.example.usernavigationhomework.MainActivity">
            <meta-data android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.usernavigationhomework.MainActivity"/>
        </activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>